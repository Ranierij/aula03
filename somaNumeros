public class SomaNumeros {
    public static void main(String[] args) {
        int numero1 = 10; // Exemplo do primeiro número
        int numero2 = 20; // Exemplo do segundo número
        
        imprimeSoma(numero1, numero2);
    }
    
    public static void imprimeSoma(int num1, int num2) {
        int soma = num1 + num2;
        System.out.println("A soma dos números é: " + soma);
    }
}
